@model List<Event_Management_System.Models.Year_Tbl>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Mst.cshtml";
}

<div class="row mt-5">
    <div class="col-md-6">
        <h3>Year Details</h3>
    </div>
    <div class="col-md-6 text-end">
        <a href="/Year/Add" class="btn btn-primary">Add New</a>
    </div>
</div>

<table class="table table-bordered">
    <tr>
        <th>Year Id</th>
        <th>Year</th>
        @*<th>Created Date</th>
        <th>Created By</th>*@
        <th>Action</th>
    </tr>

    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@Model[i].Year_id</td>
            <td>@Model[i].Year</td>
            @*<td>@Model[i].Created_Date</td>
            <td>@Model[i].Created_by</td>*@
            <td>
                <a href="/Year/Edit?Id=@Model[i].Year_id">Edit</a>
                <a href="/Year/Remove?Id=@Model[i].Year_id">Delete</a>
            </td>
        </tr>
    }
</table>


