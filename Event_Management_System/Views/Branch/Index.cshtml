@model List<Event_Management_System.Models.Branch_Tbl>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Mst.cshtml";
}



<div class="row mt-5">
    <div class="col-md-6">
        <h3>Branch Details</h3>
    </div>
    <div class="col-md-6 text-end">
        <a href="/Branch/Add" class="btn btn-primary">Add New</a>
    </div>
</div>

<table class="table table-bordered">
    <tr>
        <th>Branch_id</th>
        <th>Branch</th>
        @*<th>Created_Date</th>
        <th>Created_by</th>*@
        <th>Actions</th>
    </tr>

    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@Model[i].Branch_id</td>
            <td>@Model[i].Branch</td>
            @*<td>@Model[i].Created_Date</td>
            <td>@Model[i].Created_by</td>*@
            <td>
                <a href="/Branch/Edit?id=@Model[i].Branch_id">Edit</a>
                <a href="/Branch/Remove?id=@Model[i].Branch_id">Delete</a>
            </td>
        </tr>
    }
</table>


