@model List<Event_Management_System.Models.Booking_SeatInfo_tbl>
@{
    ViewBag.Title = "Index";
}

<div class="row mt-5">
    <div class="col-md-6">
        <h3>Book Seat Info Details</h3>
    </div>
    <div class="col-md-6 text-end">
        <a href="/Booking_SeatInfo/Add" class="btn btn-primary">Add New</a>
    </div>
</div>

<table class="table table-bordered">
    <tr>
        <th>Booking Seat Id</th>
        
        @*<th>Created Date</th>
        <th>Created By</th>*@
        <th>Action</th>
    </tr>

    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@Model[i].bookingSeat_id</td>
            
            @*<td>@Model[i].Created_Date</td>
            <td>@Model[i].Created_by</td>*@
            <td>
                <a href="/Booking_SeatInfo/Edit?Id=@Model[i].bookingSeat_id">Edit</a>
                <a href="/Booking_SeatInfo/Remove?Id=@Model[i].bookingSeat_id">Delete</a>
            </td>
        </tr>
    }
</table>

